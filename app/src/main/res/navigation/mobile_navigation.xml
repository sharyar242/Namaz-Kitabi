<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_shahada"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/nav_shahada"
        android:name="uz.texnopos.namaz.ui.shahada.ShahadaFragment"
        android:label="@string/menu_shahada"
        tools:layout="@layout/fragment_shahada" />

    <fragment
        android:id="@+id/nav_paklik"
        android:name="uz.texnopos.namaz.ui.paklik.PaklikService"
        android:label="@string/menu_paklik"
        tools:layout="@layout/fragment_service">
        <action
            android:id="@+id/action_nav_paklik_to_paklikFragment"
            app:destination="@id/paklikFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_namaz"
        android:name="uz.texnopos.namaz.ui.namaz.namazAdapter.NamazService"
        android:label="@string/menu_namaz"
        tools:layout="@layout/fragment_namaz_service">
        <action
            android:id="@+id/action_nav_namaz_to_namazFragment"
            app:destination="@id/namazFragment" />
        <action
            android:id="@+id/action_nav_namaz_to_parizNamazFragment"
            app:destination="@id/parizNamazService" />
    </fragment>

    <fragment
        android:id="@+id/namazFragment"
        android:name="uz.texnopos.namaz.ui.namaz.NamazFragment"
        android:label="NamazFragment"
        tools:layout="@layout/fragment_namaz">
        <argument
            android:name="namaz"
            android:defaultValue="1"
            app:argType="integer" />
        <argument
            android:name="type"
            android:defaultValue="1"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/paklikFragment"
        android:name="uz.texnopos.namaz.ui.paklik.PaklikFragment"
        android:label="PaklikFragment"
        tools:layout="@layout/fragment_paklik">
        <argument
            android:name="id"
            android:defaultValue="1"
            app:argType="integer" />
    </fragment>


    <fragment
        android:id="@+id/surelerFragment"
        android:name="uz.texnopos.namaz.ui.sureler.SurelerFragment"
        android:label="SurelerFragment"
        tools:layout="@layout/fragment_sureler">
        <argument
            android:name="id"
            android:defaultValue="1"
            app:argType="integer" />
        <argument
            android:name="type"
            android:defaultValue="1"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/nav_sureler"
        android:name="uz.texnopos.namaz.ui.sureler.SurelerService"
        android:label="Қысқа сүрелер"
        tools:layout="@layout/fragment_service">
        <action
            android:id="@+id/action_surelerService_to_surelerFragment"
            app:destination="@id/surelerFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_About"
        android:name="uz.texnopos.namaz.ui.about.AboutFragment"
        android:label="AboutFragment"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/nav_dualar"
        android:name="uz.texnopos.namaz.ui.dualar.DualarFragment"
        android:label="DualarFragment"
        tools:layout="@layout/fragment_dualar" />

    <fragment
        android:id="@+id/nav_40Parz"
        android:name="uz.texnopos.namaz.ui.qiriqParz.QiriqParzFragment"
        android:label="QiriqParzFragment"
        tools:layout="@layout/fragment_qiriq_parz" />
    <fragment
        android:id="@+id/parizNamazService"
        android:name="uz.texnopos.namaz.ui.parizNamaz.adapter.ParizNamazService"
        android:label="Парыз намазлар"
        tools:layout="@layout/pariz_namaz_service">
        <argument
            android:name="parizNamazId"
            app:argType="integer"
            android:defaultValue="1" />
        <argument
            android:name="parizNamazType"
            app:argType="integer"
            android:defaultValue="1" />
        <action
            android:id="@+id/action_parizNamazService_to_parizNamazFragment"
            app:destination="@id/parizNamazFragment" />

    </fragment>
    <fragment
        android:id="@+id/parizNamazFragment"
        android:name="uz.texnopos.namaz.ui.parizNamaz.ParizNamazFragment"
        android:label="ParizNamazFragment"
        tools:layout="@layout/pariz_namaz_fragment">
        <argument
            android:name="parizId"
            app:argType="integer"
            android:defaultValue="1" />
    </fragment>


</navigation>